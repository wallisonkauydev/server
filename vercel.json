{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ],
  "functions": {
    "src/main.ts": {
      "memory": 3008,
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "maxDuration": 30
    }
  }
}
